{
	"include": [
		"**/*.js",
	],
	"exclude": [
		"node_modules",
	],
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"types": ["vite/client"],
		// "strict": true,
	}
}